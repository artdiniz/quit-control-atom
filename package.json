{
  "name": "quit-control",
  "displayName": "Quit Control for Atom",
  "description": "Stop mistyping keyboard shortctus and quiting Atom unintentionally",
  "main": "./lib/quit-control-atom",
  "version": "1.0.0",
  "keywords": [
    "quit",
    "close",
    "keybinding",
    "close window",
    "close tab"
  ],
  "activationCommands": {
    "atom-workspace": [
      "quit-control-atom:quit",
      "quit-control-atom:closeWindow",
      "quit-control-atom:closeEditor"
    ]
  },
  "repository": "https://github.com/artdiniz/quitControlAtom",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-space-pen-views": "^2.2.0"
  }
}
